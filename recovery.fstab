# logical partitions
system						/system		ext4	ro,barrier=1,discard	wait,avb=vbmeta,logical,first_stage_mount
vendor						/vendor		ext4	ro,barrier=1,discard	wait,avb,logical,first_stage_mount
product						/product	ext4	ro,barrier=1,discard	wait,avb,logical,first_stage_mount
odm						/odm		ext4	ro,barrier=1,discard	wait,avb,logical,first_stage_mount

# physical partitions
/dev/block/bootdevice/by-name/boot		/boot		emmc	defaults		recoveryonly
/dev/block/bootdevice/by-name/recovery		/recovery	emmc	defaults		recoveryonly
/dev/block/bootdevice/by-name/metadata		/metadata	ext4	defaults		wait,first_stage_mount
/dev/block/bootdevice/by-name/userdata		/data		f2fs	defaults		recoveryonly
/dev/block/bootdevice/by-name/cache		/cache		ext4	defaults		recoveryonly
/dev/block/bootdevice/by-name/misc		/misc		emmc	defaults		recoveryonly
/dev/block/bootdevice/by-name/vbmeta_samsung	/vbmeta_samsung	emmc	defaults		recoveryonly
/dev/block/bootdevice/by-name/vbmeta_system	/vbmeta_system	emmc	defaults		recoveryonly

